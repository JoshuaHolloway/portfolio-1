{
  "name": "portfolio-1",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a portfolio----1",
    "rollbackh": "heroku run knex migrate:rollback -a portfolio----1",
    "databaseh": "heroku pg:psql -a portfolio----1",
    "seedh": "heroku run knex seed:run -a portfolio----1",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "deploy": "git push heroku main",
    "heroku-postbuild": "cd client && npm i && npm run build"
  },
  "engines": {
    "node": "14.18.1"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "knex": "0.95.11",
    "knex-cleaner": "1.3.1",
    "pg": "8.7.1",
    "express-sslify": "^1.2.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "nodemon": "2.0.14",
    "supertest": "6.1.6"
  },
  "keywords": [],
  "author": "josh holloway"
}
